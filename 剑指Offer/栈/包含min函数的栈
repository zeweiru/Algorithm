# -*- coding:utf-8 -*-
class Solution:
    def __init__(self):
        self.stack = []
        self.minstack = []
        self.minm = float('inf')
    def push(self, node):
        # write code here
        if node < self.minm:
            self.minm = node
            self.minstack.append(self.minm)
        self.stack.append(node)
    def pop(self):
        # write code here
        if self.stack != []:
            if self.stack[-1] == self.minm:
                self.minstack.pop()
            self.stack.pop()
    def top(self):
        # write code here
        if self.stack != []:
            return self.stack[-1]
        else:
            return None
    def min(self):
        # write code here
        return self.minstack[-1]
